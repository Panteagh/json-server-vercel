{
  "functions": {
    "api/server.js": {
      "memory": 1024,
      "includeFiles": "db.json"
      
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "api/server.js"
    },
    {
      "source": "/avatars/(.*)",
      "destination": "/public/avatars/$1"
    },
    {
      "source": "/marketPlaceNFTs/(.*)",
      "destination": "/public/marketPlaceNFTs/$1"
    }
  ]

}
